<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="by.javastudy.hibernate.dao.ContactTelDetailEntity" table="contact_tel_detail" schema="hibernatedb">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="telType">
            <column name="tel_type" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="telNumber">
            <column name="tel_number" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="version">
            <column name="version" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>